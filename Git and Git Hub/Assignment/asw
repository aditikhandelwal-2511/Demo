[32m+[m[32m'use strict'[m
[32m+[m
[32m+[m[32mconst switcher = document.querySelector('.btn');[m
[32m+[m
[32m+[m[32mswitcher.addEventListener('click', function() {[m
[32m+[m[32m    document.body.classList.toggle('dark-theme')[m
[32m+[m
[32m+[m[32m    var className = document.body.className;[m
[32m+[m[32m    if(className == "light-theme")[m
[32m+[m[32m    {[m
[32m+[m[32m        this.textContent = "Dark";[m
[32m+[m[41m        [m
[32m+[m[32m    }[m
[32m+[m[41m    [m
[32m+[m[32m    else {[m
[32m+[m[32m        this.textContent = "Light";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m});[m
